{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/lib/db/entity/**/*.ts"
   ],
   "migrations": [
      "src/lib/db/migration/**/*.ts"
   ],
   "subscribers": [
      "src/lib/db/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/lib/entity",
      "migrationsDir": "src/lib/migration",
      "subscribersDir": "src/lib/subscriber"
   }
}